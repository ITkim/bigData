   color: white;
}

/* Red */
.danger {
   border-color: #f44336;
   color: red
}

.danger:hover {
   background: #f44336;
   color: white;
}

/* Gray */
.default {
   border-color: #e7e7e7;
   color: black;
}

.default:hover {
   background: #e7e7e7;
}
